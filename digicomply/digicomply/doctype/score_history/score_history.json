{
    "name": "Score History",
    "module": "DigiComply",
    "doctype": "DocType",
    "custom": 0,
    "istable": 0,
    "issingle": 0,
    "is_tree": 0,
    "editable_grid": 0,
    "track_changes": 0,
    "engine": "InnoDB",
    "naming_rule": "Expression",
    "autoname": "SCORE-.YYYY.MM.-.#####",
    "title_field": "company",
    "search_fields": "company,score_date",
    "sort_field": "score_date",
    "sort_order": "DESC",
    "description": "Historical compliance score records for trend analysis",
    "fields": [
        {
            "fieldname": "score_section",
            "fieldtype": "Section Break",
            "label": "Score Details"
        },
        {
            "fieldname": "company",
            "fieldtype": "Link",
            "label": "Company",
            "options": "Company",
            "reqd": 1,
            "in_list_view": 1,
            "in_standard_filter": 1
        },
        {
            "fieldname": "score_date",
            "fieldtype": "Date",
            "label": "Score Date",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "column_break_1",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "total_score",
            "fieldtype": "Float",
            "label": "Total Score",
            "precision": "1",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "score_grade",
            "fieldtype": "Select",
            "label": "Grade",
            "options": "A+\nA\nB+\nB\nC+\nC\nD\nF",
            "in_list_view": 1
        },
        {
            "fieldname": "components_section",
            "fieldtype": "Section Break",
            "label": "Score Components"
        },
        {
            "fieldname": "trn_validity_score",
            "fieldtype": "Float",
            "label": "TRN Validity (20 pts)",
            "precision": "1"
        },
        {
            "fieldname": "reconciliation_score",
            "fieldtype": "Float",
            "label": "Reconciliation (25 pts)",
            "precision": "1"
        },
        {
            "fieldname": "vat_accuracy_score",
            "fieldtype": "Float",
            "label": "VAT Accuracy (25 pts)",
            "precision": "1"
        },
        {
            "fieldname": "column_break_2",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "filing_compliance_score",
            "fieldtype": "Float",
            "label": "Filing Compliance (20 pts)",
            "precision": "1"
        },
        {
            "fieldname": "data_quality_score",
            "fieldtype": "Float",
            "label": "Data Quality (10 pts)",
            "precision": "1"
        },
        {
            "fieldname": "metrics_section",
            "fieldtype": "Section Break",
            "label": "Underlying Metrics"
        },
        {
            "fieldname": "valid_trn_percentage",
            "fieldtype": "Percent",
            "label": "Valid TRN %"
        },
        {
            "fieldname": "reconciliation_match_rate",
            "fieldtype": "Percent",
            "label": "Reconciliation Match Rate"
        },
        {
            "fieldname": "vat_error_count",
            "fieldtype": "Int",
            "label": "VAT Errors"
        },
        {
            "fieldname": "column_break_3",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "on_time_filing_rate",
            "fieldtype": "Percent",
            "label": "On-Time Filing Rate"
        },
        {
            "fieldname": "data_completeness",
            "fieldtype": "Percent",
            "label": "Data Completeness"
        },
        {
            "fieldname": "change_section",
            "fieldtype": "Section Break",
            "label": "Change from Previous",
            "collapsible": 1
        },
        {
            "fieldname": "previous_score",
            "fieldtype": "Float",
            "label": "Previous Score",
            "precision": "1"
        },
        {
            "fieldname": "score_change",
            "fieldtype": "Float",
            "label": "Change",
            "precision": "1"
        },
        {
            "fieldname": "change_direction",
            "fieldtype": "Select",
            "label": "Direction",
            "options": "\nImproved\nDeclined\nUnchanged"
        }
    ],
    "permissions": [
        {
            "role": "System Manager",
            "read": 1,
            "write": 0,
            "create": 1,
            "delete": 0
        },
        {
            "role": "DigiComply Admin",
            "read": 1,
            "write": 0,
            "create": 1,
            "delete": 0
        },
        {
            "role": "External Auditor",
            "read": 1,
            "write": 0,
            "create": 0,
            "delete": 0
        }
    ]
}
