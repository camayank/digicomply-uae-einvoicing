{
    "name": "User Session Log",
    "module": "DigiComply",
    "doctype": "DocType",
    "custom": 0,
    "istable": 0,
    "issingle": 0,
    "is_tree": 0,
    "editable_grid": 0,
    "track_changes": 0,
    "engine": "InnoDB",
    "naming_rule": "Expression",
    "autoname": "SESSION-.YYYY.-.######",
    "title_field": "user",
    "search_fields": "user,session_status,ip_address",
    "sort_field": "login_time",
    "sort_order": "DESC",
    "description": "User session tracking for security audit compliance",
    "fields": [
        {
            "fieldname": "session_section",
            "fieldtype": "Section Break",
            "label": "Session Details",
            "collapsible": 0
        },
        {
            "fieldname": "user",
            "fieldtype": "Link",
            "label": "User",
            "options": "User",
            "reqd": 1,
            "in_list_view": 1,
            "in_standard_filter": 1
        },
        {
            "fieldname": "user_full_name",
            "fieldtype": "Data",
            "label": "User Full Name",
            "read_only": 1
        },
        {
            "fieldname": "session_id",
            "fieldtype": "Data",
            "label": "Session ID",
            "unique": 1
        },
        {
            "fieldname": "column_break_1",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "session_status",
            "fieldtype": "Select",
            "label": "Status",
            "options": "Active\nExpired\nLogout\nForced Logout\nTimeout",
            "default": "Active",
            "in_list_view": 1,
            "in_standard_filter": 1
        },
        {
            "fieldname": "login_type",
            "fieldtype": "Select",
            "label": "Login Type",
            "options": "Web\nAPI\nMobile\nSSO\n2FA",
            "default": "Web",
            "in_standard_filter": 1
        },
        {
            "fieldname": "timing_section",
            "fieldtype": "Section Break",
            "label": "Timing",
            "collapsible": 0
        },
        {
            "fieldname": "login_time",
            "fieldtype": "Datetime",
            "label": "Login Time",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "logout_time",
            "fieldtype": "Datetime",
            "label": "Logout Time"
        },
        {
            "fieldname": "column_break_2",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "session_duration",
            "fieldtype": "Duration",
            "label": "Session Duration",
            "read_only": 1
        },
        {
            "fieldname": "last_activity",
            "fieldtype": "Datetime",
            "label": "Last Activity"
        },
        {
            "fieldname": "location_section",
            "fieldtype": "Section Break",
            "label": "Location & Device",
            "collapsible": 0
        },
        {
            "fieldname": "ip_address",
            "fieldtype": "Data",
            "label": "IP Address",
            "in_list_view": 1
        },
        {
            "fieldname": "geo_location",
            "fieldtype": "Data",
            "label": "Geo Location",
            "description": "City, Country (if available)"
        },
        {
            "fieldname": "column_break_3",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "user_agent",
            "fieldtype": "Small Text",
            "label": "User Agent"
        },
        {
            "fieldname": "device_type",
            "fieldtype": "Select",
            "label": "Device Type",
            "options": "\nDesktop\nMobile\nTablet\nUnknown"
        },
        {
            "fieldname": "browser",
            "fieldtype": "Data",
            "label": "Browser"
        },
        {
            "fieldname": "os",
            "fieldtype": "Data",
            "label": "Operating System"
        },
        {
            "fieldname": "security_section",
            "fieldtype": "Section Break",
            "label": "Security",
            "collapsible": 0
        },
        {
            "fieldname": "two_factor_verified",
            "fieldtype": "Check",
            "label": "2FA Verified",
            "default": "0"
        },
        {
            "fieldname": "ip_whitelisted",
            "fieldtype": "Check",
            "label": "IP Whitelisted",
            "default": "0"
        },
        {
            "fieldname": "column_break_4",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "failed_attempts_before",
            "fieldtype": "Int",
            "label": "Failed Attempts Before Login",
            "description": "Number of failed login attempts before this successful login"
        },
        {
            "fieldname": "suspicious_activity",
            "fieldtype": "Check",
            "label": "Suspicious Activity Detected",
            "default": "0"
        },
        {
            "fieldname": "activity_section",
            "fieldtype": "Section Break",
            "label": "Activity Summary",
            "collapsible": 1
        },
        {
            "fieldname": "pages_visited",
            "fieldtype": "Int",
            "label": "Pages Visited",
            "default": "0"
        },
        {
            "fieldname": "documents_created",
            "fieldtype": "Int",
            "label": "Documents Created",
            "default": "0"
        },
        {
            "fieldname": "documents_modified",
            "fieldtype": "Int",
            "label": "Documents Modified",
            "default": "0"
        },
        {
            "fieldname": "column_break_5",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "reports_generated",
            "fieldtype": "Int",
            "label": "Reports Generated",
            "default": "0"
        },
        {
            "fieldname": "data_exported",
            "fieldtype": "Int",
            "label": "Data Exports",
            "default": "0"
        },
        {
            "fieldname": "api_calls",
            "fieldtype": "Int",
            "label": "API Calls",
            "default": "0"
        },
        {
            "fieldname": "company_section",
            "fieldtype": "Section Break",
            "label": "Company Context",
            "collapsible": 1
        },
        {
            "fieldname": "default_company",
            "fieldtype": "Link",
            "label": "Default Company",
            "options": "Company"
        },
        {
            "fieldname": "companies_accessed",
            "fieldtype": "Small Text",
            "label": "Companies Accessed",
            "description": "List of companies accessed during session"
        },
        {
            "fieldname": "notes_section",
            "fieldtype": "Section Break",
            "label": "Notes",
            "collapsible": 1
        },
        {
            "fieldname": "logout_reason",
            "fieldtype": "Data",
            "label": "Logout Reason"
        },
        {
            "fieldname": "admin_notes",
            "fieldtype": "Small Text",
            "label": "Admin Notes"
        }
    ],
    "permissions": [
        {
            "role": "System Manager",
            "read": 1,
            "write": 0,
            "create": 1,
            "delete": 0,
            "export": 1
        },
        {
            "role": "DigiComply Admin",
            "read": 1,
            "write": 0,
            "create": 1,
            "delete": 0,
            "export": 1
        },
        {
            "role": "External Auditor",
            "read": 1,
            "write": 0,
            "create": 0,
            "delete": 0,
            "export": 1
        }
    ],
    "links": [],
    "actions": [],
    "default_print_format": "",
    "show_preview_popup": 0,
    "make_attachments_public": 0
}
